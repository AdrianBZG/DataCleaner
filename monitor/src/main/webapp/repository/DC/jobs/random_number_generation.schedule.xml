<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<schedule xmlns="http://eobjects.org/datacleaner/schedule/1.0" xmlns:ns2="http://eobjects.org/datacleaner/shared/1.0" xmlns:ns3="http://eobjects.org/datacleaner/timeline/1.0" xmlns:ns4="http://eobjects.org/datacleaner/execution-log/1.0">
    <cron-expression>@daily</cron-expression>
    <alerts>
        <alert>
            <description>Req. Mean between 0 and 400</description>
            <ns2:metric>
                <ns2:analyzer-descriptor-name>Number analyzer</ns2:analyzer-descriptor-name>
                <ns2:analyzer-input>Random number</ns2:analyzer-input>
                <ns2:metric-descriptor-name>Mean</ns2:metric-descriptor-name>
                <ns2:metric-param-column-name>Random number</ns2:metric-param-column-name>
            </ns2:metric>
            <minimum-value>0</minimum-value>
            <maximum-value>400</maximum-value>
            <severity>FATAL</severity>
        </alert>
        <alert>
            <description>Req. Mean between 5 and 300</description>
            <ns2:metric>
                <ns2:analyzer-descriptor-name>Number analyzer</ns2:analyzer-descriptor-name>
                <ns2:analyzer-input>Random number</ns2:analyzer-input>
                <ns2:metric-descriptor-name>Mean</ns2:metric-descriptor-name>
                <ns2:metric-param-column-name>Random number</ns2:metric-param-column-name>
            </ns2:metric>
            <minimum-value>5</minimum-value>
            <maximum-value>300</maximum-value>
            <severity>WARNING</severity>
        </alert>
        <alert>
            <description>Expect Mean from 100 to 200</description>
            <ns2:metric>
                <ns2:analyzer-descriptor-name>Number analyzer</ns2:analyzer-descriptor-name>
                <ns2:analyzer-input>Random number</ns2:analyzer-input>
                <ns2:metric-descriptor-name>Mean</ns2:metric-descriptor-name>
                <ns2:metric-param-column-name>Random number</ns2:metric-param-column-name>
            </ns2:metric>
            <minimum-value>100</minimum-value>
            <maximum-value>200</maximum-value>
            <severity>SURVEILLANCE</severity>
        </alert>
        <alert>
            <description>Expect Mean from 120 to 180</description>
            <ns2:metric>
                <ns2:analyzer-descriptor-name>Number analyzer</ns2:analyzer-descriptor-name>
                <ns2:analyzer-input>Random number</ns2:analyzer-input>
                <ns2:metric-descriptor-name>Mean</ns2:metric-descriptor-name>
                <ns2:metric-param-column-name>Random number</ns2:metric-param-column-name>
            </ns2:metric>
            <minimum-value>120</minimum-value>
            <maximum-value>180</maximum-value>
            <severity>INTELLIGENCE</severity>
        </alert>
    </alerts>
</schedule>
