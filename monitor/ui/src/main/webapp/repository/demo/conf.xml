<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://eobjects.org/analyzerbeans/configuration/1.0">
    <datastore-catalog>
        <jdbc-datastore name="orderdb" description="DataCleaner sample database, containing an example data warehouse with orders, customers, products, employees etc.">
            <url>jdbc:hsqldb:res:orderdb</url>
            <driver>org.hsqldb.jdbcDriver</driver>
            <username>SA</username>
            <password></password>
        </jdbc-datastore>
        <csv-datastore name="Vendor list" description="A growing list of known product vendor names, used as a reference dictionary in the example product profiling job to ensure that vendor names are correctly filled and spelled.">
            <filename>reference-data/vendor_dictionary.txt</filename>
        </csv-datastore>
