<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<job xmlns="http://eobjects.org/analyzerbeans/job/1.0">
    <job-metadata>
        <job-description>Created with DataCleaner 2.0 (BETA)</job-description>
        <updated-date>2011-02-09+01:00</updated-date>
    </job-metadata>
    <source>
        <data-context ref="orderdb"/>
        <columns>
            <column path="PUBLIC.ORDERS.ORDERNUMBER" id="col_0"/>
            <column path="PUBLIC.ORDERS.ORDERDATE" id="col_1"/>
            <column path="PUBLIC.ORDERS.REQUIREDDATE" id="col_2"/>
            <column path="PUBLIC.ORDERS.SHIPPEDDATE" id="col_3"/>
            <column path="PUBLIC.ORDERS.STATUS" id="col_4"/>
            <column path="PUBLIC.ORDERS.COMMENTS" id="col_5"/>
            <column path="PUBLIC.ORDERS.CUSTOMERNUMBER" id="col_6"/>
        </columns>
    </source>
    <transformation>
        <transformer name="Customer no. as string">
            <descriptor ref="Convert to string"/>
            <properties/>
            <input ref="col_6"/>
            <output id="col_8" name="Customer no."/>
        </transformer>
        <transformer name="Order no. as string">
            <descriptor ref="Convert to string"/>
            <properties/>
            <input ref="col_0"/>
            <output id="col_7" name="Order no."/>
        </transformer>
    </transformation>
    <analysis>
        <analyzer name="Gaps between orderdate and requireddate">
            <descriptor ref="Date gap analyzer"/>
            <properties>
                <property value="false" name="Count intersecting from and to dates as overlaps"/>
            </properties>
            <input name="From column" ref="col_1"/>
            <input name="To column" ref="col_2"/>
            <input name="Group column" ref="col_8"/>
        </analyzer>
        <analyzer>
            <descriptor ref="Date/time analyzer"/>
            <properties/>
            <input ref="col_1"/>
            <input ref="col_2"/>
            <input ref="col_3"/>
        </analyzer>
        <analyzer>
            <descriptor ref="Weekday distribution"/>
            <properties/>
            <input ref="col_1"/>
            <input ref="col_2"/>
            <input ref="col_3"/>
        </analyzer>
        <analyzer name="Status distribution">
            <descriptor ref="Value distribution"/>
            <properties>
                <property value="true" name="Record unique values"/>
            </properties>
            <input ref="col_4"/>
        </analyzer>
        <analyzer name="Similar sounding comments">
            <descriptor ref="Phonetic similarity finder"/>
            <properties>
                <property value="STRICT" name="Match mode"/>
            </properties>
            <input ref="col_5"/>
        </analyzer>
    </analysis>
</job>
