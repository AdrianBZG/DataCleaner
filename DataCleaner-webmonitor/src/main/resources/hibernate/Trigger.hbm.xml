<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dk.eobjects.datacleaner.webmonitor.model">
	<class name="Trigger" table="triggers">
		<id name="id">
			<generator class="increment" />
		</id>
		<property name="name" unique="true" />
		<property name="cronExpression" not-null="false" />
		<property name="repeatInterval" not-null="false" />
		<set name="profilerJobs" table="profiler_jobs_triggers" lazy="false">
			<key column="trigger_id" />
			<many-to-many class="ProfilerJob" column="profiler_job_id" />
		</set>
	</class>
</hibernate-mapping>