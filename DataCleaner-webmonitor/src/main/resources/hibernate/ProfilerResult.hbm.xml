<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dk.eobjects.datacleaner.webmonitor.model">
	<class name="ProfilerResult" table="profiler_results">
		<id name="id">
			<generator class="increment" />
		</id>
		<property name="profileName" />
		<property name="tableName" />
		<property name="date" />
		<many-to-one name="profilerJob" column="profiler_job_id">
		</many-to-one>
		<set name="cells" lazy="false" cascade="all">
			<key column="profiler_result_id" />
			<one-to-many class="ProfilerResultCell" />
		</set>
		<array name="columnNames" table="profiler_result_columns">
			<key column="profiler_result_id" />
			<list-index column="column_index" />
			<element column="column_name" type="string" />
		</array>
		<array name="rowNames" table="profiler_result_rows">
			<key column="profiler_result_id" />
			<list-index column="row_index" />
			<element column="row_name" type="string" />
		</array>
	</class>
</hibernate-mapping>