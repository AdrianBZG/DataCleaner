       xmlns:remconf="http://www.datacleaner.org/schema/remoteComponentsConfiguration"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.datacleaner.org/schema/remoteComponentsConfiguration http://www.datacleaner.org/schema/remoteComponentsConfiguration.xsd">


    <remconf:published-components id="published-components">
