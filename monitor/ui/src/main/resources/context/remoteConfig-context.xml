        </remconf:excludes>
        <remconf:defaults>
            <remconf:component name="Component1">
                <remconf:properties>
                    <remconf:property name="property1">
                        AAAAA
                    </remconf:property>

                    <!--<remconf:property name="property2">-->
                    <!--BBBBB-->
                    <!--</remconf:property>-->

                    <remconf:propertyXML name="propertyxml">
                        <a>
                            aaaaa
                        </a>
                    </remconf:propertyXML>
                </remconf:properties>
            </remconf:component>
            <remconf:component name="component2">
                <remconf:properties></remconf:properties>
            </remconf:component>
        </remconf:defaults>
    </remconf:published-components>
</beans>