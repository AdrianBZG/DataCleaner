<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!--
		Profiler modules. If you have additional profiles to add,
		add descriptors for them here.
	-->
	
	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="Standard measures" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.trivial.StandardMeasuresProfile" />
		<property name="iconPath" value="images/profile_standard_measures.png" />
	</bean>

	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="String analysis" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.trivial.StringAnalysisProfile" />
		<property name="literalsRequired" value="true" />
		<property name="iconPath" value="images/profile_string_analysis.png" />
	</bean>

	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="Time analysis" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.trivial.TimeAnalysisProfile" />
		<property name="datesRequired" value="true" />
		<property name="iconPath" value="images/profile_time_analysis.png" />
	</bean>

	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="Number analysis" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.trivial.NumberAnalysisProfile" />
		<property name="numbersRequired" value="true" />
		<property name="iconPath" value="images/profile_number_analysis.png" />
	</bean>

	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="Pattern finder" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.pattern.PatternFinderProfile" />
		<property name="iconPath" value="images/profile_pattern_finder.png" />
	</bean>

	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="Dictionary matcher" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.trivial.DictionaryProfile" />
		<property name="literalsRequired" value="true" />
		<property name="iconPath" value="images/dictionaries.png" />
	</bean>
	
	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="Regex matcher" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.trivial.RegexProfile" />
		<property name="iconPath" value="images/regexes.png" />
	</bean>
	
	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="Date mask matcher" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.trivial.DateMaskProfile" />
		<property name="literalsRequired" value="true" />
		<property name="iconPath" value="images/profile_datemask.png" />
	</bean>

	<bean class="dk.eobjects.datacleaner.profiler.BasicProfileDescriptor">
		<property name="displayName" value="Value distribution" />
		<property name="profileClass" value="dk.eobjects.datacleaner.profiler.valuedist.ValueDistributionProfile" />
		<property name="iconPath" value="images/profile_repeated_values.png" />
	</bean>
</beans>