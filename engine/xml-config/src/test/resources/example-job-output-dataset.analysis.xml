<?xml version="1.0" encoding="UTF-8"?>
<job xmlns="http://eobjects.org/analyzerbeans/job/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<source>
		<data-context ref="my database" />
		<columns>
			<column id="col_fn" path="PUBLIC.EMPLOYEES.FIRSTNAME" />
			<column id="col_ln" path="PUBLIC.EMPLOYEES.LASTNAME" />
			<column id="col_supervisor" path="PUBLIC.EMPLOYEES.REPORTSTO" />
		</columns>
	</source>

	<transformation>
		<transformer>
			<descriptor ref="Concatenator" />
			<properties>
				<property name="Separator" value=" " />
			</properties>
			<input ref="col_fn" />
			<input ref="col_ln" />
			<output id="col_fullname" />
		</transformer>
	</transformation>

	<analysis>
		<analyzer requires="_any_">
			<descriptor ref="Character set distribution" />
			<input ref="col_fullname" />
			<output-data-stream name="character sets">
				<job>
					<source>
						<columns>
							<column id="col_charset" path="character sets.Character set"/>
							<column id="col_name" path="character sets.Concat of FIRSTNAME,LASTNAME"/>
						</columns>
					</source>
					<analysis>
						<analyzer>
							<descriptor ref="String analyzer"/>
							<input ref="col_charset" />
						</analyzer>
						<analyzer>
							<descriptor ref="Number analyzer"/>
							<input ref="col_name" />
						</analyzer>
					</analysis>
				</job>
			</output-data-stream>
		</analyzer>
	</analysis>

</job>